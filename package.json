{
  "name": "guesstimate",
  "version": "0.4.0",
  "description": "Make, share, and evaluate estimates of things",
  "main": "index.js",
  "scripts": {
    "_copy_assets": "mkdir -p public && cp src/_redirects public/_redirects && cp src/static/* public/",
    "predeploy": "yarn && BUILD_DEV=0 API_ENV=production webpack && yarn _copy_assets",
    "_stage": "surge -p public -d stage.getguesstimate.com",
    "ci:test": "yarn test-once -- --browsers ChromeHeadless",
    "lint": "eslint . --quiet",
    "start": "yarn _copy_assets && BUILD_DEV=1 NODE_ENV=development API_ENV=development webpack-dev-server",
    "start-production": "BUILD_DEV=1 NODE_ENV=development API_ENV=production webpack-dev-server",
    "stage": "yarn predeploy && yarn _stage",
    "stage-master": "yarn predeploy && surge -p public -d master.getguesstimate.com",
    "stage-ozzie": "yarn predeploy && surge -p public -d ozzie.getguesstimate.com",
    "stage-matthew": "yarn predeploy && surge -p public -d matthew.getguesstimate.com",
    "stage-factbank": "yarn predeploy && surge -p public -d factbank.getguesstimate.com",
    "test": "jest",
    "deploy": "yarn predeploy && netlify deploy -e production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guesstimate/guesstimate-app.git"
  },
  "keywords": [
    "Monte Carlo",
    "Estimate",
    "Guesstimate"
  ],
  "author": "Ozzie Gooen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/getguesstimate/guesstimate-app/issues"
  },
  "homepage": "https://www.getguesstimate.com",
