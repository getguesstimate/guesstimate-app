{
  "name": "guesstimate",
  "version": "0.4.0",
  "description": "Make, share, and evaluate estimates of things",
  "main": "index.js",
  "scripts": {
    "_build": "BUILD_DEV=0 API_ENV=production webpack",
    "_stage": "surge -p public -d test.getguesstimate.com",

    "start": "BUILD_DEV=0 NODE_ENV=development API_ENV=development webpack-dev-server",
    "start-production": "BUILD_DEV=0 NODE_ENV=development API_ENV=production webpack-dev-server",

    "stage!": "BUILD_DEV=0 npm run _build && npm run _stage",
    "stage-payments!": "BUILD_DEV=0 npm run _build && surge -p public -d payments.getguesstimate.com",
    "deploy": "surge -p public -d https://www.getguesstimate.com",

    "test": "karma start",
    "test-once": "karma start --single-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guesstimate/guesstimate-app.git"
